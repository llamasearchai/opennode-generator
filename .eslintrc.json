{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended"],
  "plugins": ["@typescript-eslint"],
  "env": {
    "node": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "off",
    "no-empty": "warn",
    "no-useless-escape": "warn",
    "no-undef": "off"
  }
}
